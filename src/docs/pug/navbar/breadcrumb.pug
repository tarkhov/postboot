extends ../example.pug

block head_title
  +example_head_title('navbar', 'breadcrumb')

block meta_description
  +example_meta_description('navbar', 'breadcrumb')

block breadcrumb
  +example_breadcrumb('navbar')

block example_description
  +example_description('navbar', 'breadcrumb')

block example_result
  nav.navbar.navbar-expand.navbar-dark.bg-dark.px-0
    .container-fluid
      = "\n  "
      a.navbar-brand.d-none.d-sm-inline-block(href="#") Brand
      ol.breadcrumb.breadcrumb-dark.bg-transparent.px-0.navbar-breadcrumb
        li.breadcrumb-item: a.breadcrumb-link(href='#') Home
        li.breadcrumb-item: a.breadcrumb-link(href='#') Category
        li.breadcrumb-item.active Article

